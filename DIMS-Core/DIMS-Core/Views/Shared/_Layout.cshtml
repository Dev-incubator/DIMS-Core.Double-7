<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - DIMS - .Net Core</title>
    <link type="image/png" rel="shortcut icon" href="~/favicon.ico" />
    <link rel="stylesheet" href="~/bundle/css/main.bundle.min.css" />
    @*<script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/chosen/1.1.0/chosen.jquery.min.js"></script>
    <link rel="stylesheet" type="text/css" href="https://cdnjs.cloudflare.com/ajax/libs/chosen/1.1.0/chosen.min.css">*@
    <script src="~/bundle/js/main.bundle.min.js"></script>
</head>
<body>
    <partial name="_Spinner" />
    <partial name="_Header" />
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>
    <partial name="_Footer" />
    
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
